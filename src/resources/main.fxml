<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pack.Controller">
   <children>
      <HBox>
         <children>
            <Button minWidth="100.0" mnemonicParsing="false" text="Button">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="CleanText" maxHeight="1.7976931348623157E308" minWidth="100.0" mnemonicParsing="false" onAction="#cleanText" text="Delete" />
            <TextField fx:id="sendAdr" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <Button minWidth="100.0" mnemonicParsing="false" onAction="#sendHandler" text="Send">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <SplitPane dividerPositions="0.4649122807017544" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <items>
            <TextArea fx:id="serverMsg" prefHeight="200.0" prefWidth="200.0" wrapText="true">
               <font>
                  <Font size="16.0" />
               </font>
            </TextArea>
            <TextArea fx:id="clientMsg" prefHeight="200.0" prefWidth="200.0" wrapText="true">
               <font>
                  <Font size="16.0" />
               </font>
            </TextArea>
         </items>
      </SplitPane>
   </children>
</VBox>
